{
  "name": "dwdm-manager-ui",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/webServer.js",
  "description": "DWDM Manager UI",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && npm run webpack-prod && npm run build-prod-server",
    "webpack": "cross-env NODE_ENV=development node ./node_modules/webpack/bin/webpack --config webpack.dev.js --progress",
    "webpack-prod": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.prod.js --progress",
    "coverage": "nyc npm run test",
    "clean": "del-cli dist",
    "serve": "webpack-dev-server --open --config webpack.dev.js",
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node ./dist/proxy/index.js"
  },
